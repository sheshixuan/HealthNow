    <script>
        var MySelection = {}; // Globally scoped object

        function GenderButton(IdBaseName, NumberOfButtons) {
            CheckThisButton = IdBaseName + NumberOfButtons;
            ThisDiv = IdBaseName + NumberOfButtons + 'Div';
            This = IdBaseName + NumberOfButtons
            y = 3 - NumberOfButtons
            OtherDiv = IdBaseName + y + 'Div';
            Other = IdBaseName + y
            document.getElementById(ThisDiv).style.display = "block";
            document.getElementById(This).classList.add('onclick');
            document.getElementById(OtherDiv).style.display = "none";
            document.getElementById(Other).classList.remove('onclick');
            MySelection.Gender = CheckThisButton;
        }

        function AgeGroupButton(IdBaseName, NumberOfButtons) {
            for (x = 1; x <= 6; x++) {
                This = IdBaseName + x;
                if (x == NumberOfButtons) {
                    MySelection.AgeGroup = This
                    document.getElementById(This).classList.add('onclick');
                }
                else {
                    document.getElementById(This).classList.remove('onclick');
                }
            }
        }

        function LifestyleButton(IdBaseName, NumberOfButtons) {
            if (MySelection.Lifestyle) {
                LastDiv = MySelection.Gender + MySelection.AgeGroup + MySelection.Lifestyle + 'Div';
                document.getElementById(LastDiv).style.display = "none";
            }
            for (x = 1; x <= 3; x++) {
                This = IdBaseName + x;
                if (x == NumberOfButtons) {
                    MySelection.Lifestyle = This
                    document.getElementById(This).classList.add('onclick');
                }
                else {
                    document.getElementById(This).classList.remove('onclick');
                }
            }
            ThisDiv = MySelection.Gender + MySelection.AgeGroup + MySelection.Lifestyle + 'Div';
            MySelection.calorie = MySelection.Gender + MySelection.AgeGroup + MySelection.Lifestyle;
            document.getElementById(ThisDiv).style.display = "block";
        }

        function BodyTypeButton(IdBaseName, NumberOfButtons) {
            for (x = 1; x <= 5; x++) {
                This = IdBaseName + x;
                ThisDiv = IdBaseName + x + 'Div';
                if (x == NumberOfButtons) {
                    document.getElementById(ThisDiv).style.display = "block";
                    document.getElementById(This).classList.add('onclick');
                    MySelection.Bodytype = This
                }
                else {
                    document.getElementById(ThisDiv).style.display = "none";
                    document.getElementById(This).classList.remove('onclick');
                }
            }
            return false;
        }

